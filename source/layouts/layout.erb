<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <title><%= data.page.title || "Escuela Web" %></title>
    <link href='http://fonts.googleapis.com/css?family=Dosis:400,600|Ubuntu:300,400,700|Open+Sans:400,300,600|Chivo:400,900' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag "app" %>
  </head>
  <body>
    <div class="fixed header">
      <%= partial "header" %>
    </div>
    
    <div class="over_content">
      <div class="container">
        <%= partial "over_content" %>
      </div>
    </div>

    <div id="content" class="container">
      <div id="main" class="row">
        <%= yield %>
      </div>
    </div>

    <div id="footer" class="footer">
      <div class="upperfooter">
        <div class="container">
          
        </div>
      </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
    <%= javascript_include_tag  "all" %>
  </body>
</html>